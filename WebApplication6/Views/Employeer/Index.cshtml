@using WebApplication6.Models;
@{
    Layout = "_Layout";
}
@model List<Employee>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Employee id</th>
            <th scope="col"> Employee Name</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <th scope="row">@i</th>
                <td>@Model[i].Id</td>
                <td><a href="" class="text-decoration-none text-black">@Model[i].Name</a></td>
            </tr>
        }
    </tbody>
</table>
